   class Node<T>{
    T? value;
    Node<T>? next;
    Node<T>? previous;

    Node(T value ){
        this.value=value;
     next=null;
     previous=null;

    }
    }

class playist<T>{
Node<T>? head;
Node<T>? tail;
String name="";

 
playist(String name ){
this.name=name;
head=null;
tail=null;
}


void addSong(T track){
if (head==null){
    head=new Node<T>(track);
    tail=head;
}
else{
Node<T>? check= head; 
while(check!.next!=null){
check=check.next;}
check.previous=check;
check=check.next;
check?.next=null;
}

}
 void removeSong(T track){
    
    if(head!.value==track){
        if(head!.next==null){
            head=null;

        }
        else{
            head=head!.next;
            head!.previous=null;
        }


    }
    else{
Node<T>? check= head; 
while(check!.next!.value!=track){
check=check.next;
}
if(check.next!=null){
    if(check!.next!.next!=null){
        
        check.next=check.next!.next;
        check.next!.next!.previous=check;
    
    }
    else{
        check.next=null;
    }
}
}
 }


 void printNode(){
Node<T>? check= head; 
while(check!.next!=null){
print (check.value);
check=check.next;}

}

 }
